
<% @posts.each do |post| %>
    <h2><%= post.title %></h2>
    
    
    <%= markdown(post.content) %>
    

    <%= link_to 'Show', post %>
    <%= link_to 'Edit', edit_post_path(post) %>
    <%= link_to 'Destroy', post, :confirm => 'Are you sure?',
                                     :method => :delete %>
<% end %>
 
